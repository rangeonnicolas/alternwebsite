CREATE TABLE "api2_source_authors" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "source_id" integer NOT NULL REFERENCES "api2_source" ("id"), "author_id" integer NOT NULL REFERENCES "api2_author" ("entity_ptr_id"), UNIQUE ("source_id", "author_id"));
